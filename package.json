{
	"name": "DOD",
	"version": "0.0.1",
	"description": "WordPress, Docker, Webpack, Win.",
	"browserslist": [
		">1%",
		"last 4 versions",
		"Firefox ESR",
		"not ie < 9"
	],
	"scripts": {
		"start": "cross-env NODE_ENV=development node internals/scripts/start.js",
		"build": "cross-env NODE_ENV=production node internals/scripts/build.js",
		"dist": "cross-env NODE_ENV=production node internals/scripts/build.js && internals/scripts/deploy/production.sh",
		"staging": "cross-env NODE_ENV=production node internals/scripts/build.js && internals/scripts/deploy/staging.sh",
		"postinstall": "npm run build",
		"lint:staged": "lint-staged",
		"plop": "plop"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"src/**/*.{js,jsx,ts,tsx,json,css,md}": [
			"prettier --write"
		],
		"src/style.scss": [
			"prettier --write"
		]
	},
	"author": "jaredpalmer",
	"license": "MIT",
	"dependencies": {
		"@barba/core": "^2.9.7",
		"@tailwindcss/aspect-ratio": "^0.3.0",
