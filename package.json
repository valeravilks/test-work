		"config": "^3.3.3",
		"drawsvg": "^1.1.0",
		"google-maps": "^4.3.3",
		"gsap": "^3.5.1",
		"headroom.js": "^0.12.0",
		"iframe-resizer": "^4.2.11",
		"jquery-datepicker": "^1.12.3",
		"lazysizes": "^5.2.2",
		"lg-autoplay": "^1.2.1",
		"lg-fullscreen": "^1.2.1",
		"lg-hash": "^1.0.4",
		"lg-pager": "^1.0.2",
		"lg-share": "^1.2.1",
		"lg-thumbnail": "^1.2.1",
		"lg-video": "^1.3.0",
		"lg-zoom": "^1.3.0",
		"lightgallery": "^1.9.0",
		"lottie-web": "^5.7.3",
		"magnific-popup": "^1.1.0",
		"micromodal": "^0.4.6",
		"picturefill": "^3.0.3",
		"postcss": "^8.3.11",
		"rellax": "^1.12.1",
		"scrolltofixed": "^1.0.6",
		"simpleweather": "^3.1.0",
		"swiper": "^6.3.4",
		"vivus": "^0.4.5",
		"waypoints": "^4.0.1",
		"wow.js": "^1.2.2"
